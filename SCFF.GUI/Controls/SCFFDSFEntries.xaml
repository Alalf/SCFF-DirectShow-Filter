<UserControl x:Class="SCFF.GUI.Controls.SCFFDSFEntries"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" d:DesignWidth="319.708" d:DesignHeight="31.752"
             FontSize="9">
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition/>
      <ColumnDefinition Width="55"/>
    </Grid.ColumnDefinitions>
    <Grid.RowDefinitions>
      <RowDefinition Height="25"/>
    </Grid.RowDefinitions>
    <ComboBox VerticalAlignment="Center" Height="20" Margin="5,0"
              IsEnabled="False"
              x:Name="Processes">
      <ComboBoxItem>Process1</ComboBoxItem>
      <ComboBoxItem>Process2</ComboBoxItem>
      <ComboBoxItem>Process3</ComboBoxItem>
    </ComboBox>
    <Button Grid.Column="1"
            VerticalAlignment="Center" Height="20" Margin="0,0,5,0"
            x:Name="Refresh" Content="Refresh" 
            Click="Refresh_Click"/>
  </Grid>
</UserControl>
